{
  "expo": {
    "name": "MealsToGo",
    "slug": "MealsToGo",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(MealsToGo) to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "MealsToGo"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hamdisakkaf.MealsToGo",
      "config": {
              "googleMaps": {
               "apiKey": "AIzaSyDVzniVMkcQODAVvSZ7zN0Ee9qxiIsO_EY"
              }
            }
    },
    "extra": {
      "eas": {
        "projectId": "da0b3aeb-a59c-48fe-ba84-35416465608d"
      }
    }
  }
}
